<?xml version="1.0"?>
<?cocoon-process type="xslt"?>
<!DOCTYPE nete:proxyrules SYSTEM "file:////opt/CA/secure-proxy/proxy-engine/conf/dtd/proxyrules.dtd">
<!-- Proxy Rules-->
<nete:proxyrules xmlns:nete="http://www.google.com/">
	<nete:cond type="uri" criteria="beginswith">
		<nete:case value="/protected/1">
			<nete:forward>http://www.google.com$1</nete:forward>
		</nete:case>
		<nete:case value="/protected/2">
			<nete:forward>http://www.ca.com$1</nete:forward>
		</nete:case>
	</nete:cond>
</nete:proxyrules>
