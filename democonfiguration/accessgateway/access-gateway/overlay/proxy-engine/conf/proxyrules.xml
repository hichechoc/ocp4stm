<!DOCTYPE nete:proxyrules SYSTEM "file:///$$PROXY_RULES_DTD$$">

<!-- Proxy Rules -->
<!-- replace www.example.com with your namespace -->
<nete:proxyrules xmlns:nete="http://www.example.com/">
   <nete:cond criteria="beginswith" type="uri">
      <!-- replace /dir1 with an appropriate URI -->
      <nete:case value="/protected">
         <!-- replace http://server1.example.com with the appropriate destination server -->
         <nete:forward>http://www.ca.com$2</nete:forward>
      </nete:case>
      <nete:default>
         <!-- replace http://home.example.com with a default destination used for all requests that do not match the nete:case elements -->
         <nete:forward>http://www.google.com$2</nete:forward>
      </nete:default>
   </nete:cond>
</nete:proxyrules>
